<section>
  <div class="container" id="listing"> 
    <h1>Available listings</h1>
      {{#each listings}}
<div class="card-main">

    <div class="card" style="width: 30rem;">

        <img class="card-img-top" src="{{this.photo}}" alt="Card image cap" width="200" height="200">
        <div class="card-body">
          <h5 class="card-title"> <a href="/rentals/{{this._id}}">{{ this.name }}</a></h5>
          <p class="card-text">{{this.description}}</p>
          <a href="/rentals/{{this._id}}" class="btn btn-primary">Learn More..</a>
        </div>
      </div>
