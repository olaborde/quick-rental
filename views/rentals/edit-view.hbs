<form action="/rentals/update/{{ listing._id }}" method="post" enctype="multipart/form-data">
    
    <label for="">Name</label>
    <input type="text" name="editedName" id="" value="{{ listing.name }}">
    <br>
    <label for="">Address:</label>
    <input type="text" name="editedAddress" id="" value="{{ listing.address}}">
    <br>
    <label for="">Photo</label>
    <input type="file" name="editedPhoto" id="">
    <br>
    <label for="">Description:</label>
    <input type="text" name="editedDescription" id="" value="{{ listing.description}}">
    <br>
    <label for="">Availability:</label>
    <input type="text" name="editedAvailability" id="" value="{{ listing.availability}}">
    <br>
    {{!-- <label for="">Owner:</label>
    <input type="text" name="editedOwner" id="" value="{{listings.listingOwner}}}"> --}}
    <br>
    <button type="submit">Save changes</button>

</form>



